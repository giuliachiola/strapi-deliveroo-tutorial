{
  "connection": "default",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "address": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "dishes": {
      "type": "json"
    },
    "amount": {
      "type": "decimal"
    },
    "user": {
      "model": "permission",
      "via": "orders",
      "plugin": "users-permissions"
    }
  }
}